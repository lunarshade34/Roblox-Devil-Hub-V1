local speedEnabled = false
local jumpEnabled = false
local wallhackEnabled = false
local aimbotEnabled = false
local ddosActive = false

local originalSpeed = 16
local originalJump = 50
local currentSpeed = 120
local currentJump = 250
local aimbotRadius = 150
local aimbotSmoothness = 0.3

local espHighlights = {}
local aimbotCircle = nil
local aimbotConnection = nil
local ddosThreads = {}

local function updateStatus(text, isError)
    StatusLabel.Text = "Status: " .. text
    StatusLabel.TextColor3 = isError and Color3.fromRGB(255, 100, 150) or Color3.fromRGB(180, 100, 255)
end

local function createAimbotCircle()
    if aimbotCircle then aimbotCircle:Destroy() end
    aimbotCircle = Drawing.new("Circle")
    aimbotCircle.Visible = false
    aimbotCircle.Thickness = 3
    aimbotCircle.NumSides = 64
    aimbotCircle.Radius = aimbotRadius
    aimbotCircle.Color = Color3.fromRGB(255, 0, 255)
    aimbotCircle.Filled = false
    aimbotCircle.Transparency = 1
    aimbotCircle.Position = Vector2.new(camera.ViewportSize.X / 2, camera.ViewportSize.Y / 2)
end

local function updateAimbotRadius(newRadius)
    aimbotRadius = newRadius
    RadiusValue.Text = tostring(math.floor(aimbotRadius))
    if aimbotCircle then
        aimbotCircle.Radius = aimbotRadius
    end
end

local function handleSliderInput(input)
    local pos = math.clamp((input.Position.X - RadiusSlider.AbsolutePosition.X) / RadiusSlider.AbsoluteSize.X, 0, 1)
    SliderButton.Position = UDim2.new(pos, 0, 0, 0)
    local newRadius = 50 + (pos * 450)
    updateAimbotRadius(newRadius)
end

SliderButton.MouseButton1Down:Connect(function()
    local connection
    connection = userInputService.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement then
            handleSliderInput(input)
        end
    end)
    
    userInputService.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            connection:Disconnect()
        end
    end)
end)

if isMobile then
    SliderButton.TouchTap:Connect(function()
    end)
    
    SliderButton.TouchLongPress:Connect(function()
    end)
end

local function getTeamColor(targetPlayer)
    if not targetPlayer or not targetPlayer.Character then return Color3.fromRGB(255, 0, 0) end
    local success, result = pcall(function()
        if player.Team and targetPlayer.Team then
            return player.Team == targetPlayer.Team
        end
        return false
    end)
    return (success and result) and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
end

local function clearESP()
    for _, highlight in pairs(espHighlights) do
        if highlight then
            pcall(function()
                highlight:Destroy()
            end)
        end
    end
    espHighlights = {}
end

local function createHighlightForPlayer(targetPlayer)
    if espHighlights[targetPlayer] or targetPlayer == player then return end
    
    if targetPlayer.Character then
        local highlight = Instance.new("Highlight")
        highlight.Name = "DevilESP"
        highlight.Parent = targetPlayer.Character
        highlight.FillColor = getTeamColor(targetPlayer)
        highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
        highlight.FillTransparency = 0.3
        highlight.OutlineTransparency = 0
        highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
        
        espHighlights[targetPlayer] = highlight
    end
    
    targetPlayer.CharacterAdded:Connect(function()
        task.wait(1)
        if wallhackEnabled and targetPlayer ~= player then
            local newHighlight = Instance.new("Highlight")
            newHighlight.Name = "DevilESP"
            newHighlight.Parent = targetPlayer.Character
            newHighlight.FillColor = getTeamColor(targetPlayer)
            newHighlight.OutlineColor = Color3.fromRGB(255, 255, 255)
            newHighlight.FillTransparency = 0.3
            newHighlight.OutlineTransparency = 0
            newHighlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
            espHighlights[targetPlayer] = newHighlight
        end
    end)
end

local function toggleWallhack(state)
    clearESP()
    
    if not state then
        updateStatus("Wallhack Disabled")
        return
    end
    
    for _, otherPlayer in pairs(game.Players:GetPlayers()) do
        if otherPlayer ~= player then
            createHighlightForPlayer(otherPlayer)
        end
    end
    
    game.Players.PlayerAdded:Connect(function(newPlayer)
        if newPlayer ~= player then
            task.wait(1)
            createHighlightForPlayer(newPlayer)
        end
    end)
    
    runService.RenderStepped:Connect(function()
        if wallhackEnabled then
            for otherPlayer, highlight in pairs(espHighlights) do
                if otherPlayer and otherPlayer.Character and highlight then
                    highlight.FillColor = getTeamColor(otherPlayer)
                end
            end
        end
    end)
    
    updateStatus("Wallhack Active")
end

local function getClosestTarget()
    local closest = nil
    local shortestDistance = aimbotRadius
    local mousePos = Vector2.new(mouse.X, mouse.Y)
    
    for _, otherPlayer in pairs(game.Players:GetPlayers()) do
        if otherPlayer ~= player and otherPlayer.Character then
            local character = otherPlayer.Character
            local humanoid = character:FindFirstChild("Humanoid")
            local head = character:FindFirstChild("Head")
            
            if humanoid and humanoid.Health > 0 and head then
                local headPos, onScreen = camera:WorldToViewportPoint(head.Position)
                
                if onScreen then
                    local screenPos = Vector2.new(headPos.X, headPos.Y)
                    local distance = (screenPos - mousePos).Magnitude
                    
                    if distance < shortestDistance then
                        local isEnemy = true
                        local success, result = pcall(function()
                            if player.Team and otherPlayer.Team then
                                return player.Team ~= otherPlayer.Team
                            end
                            return true
                        end)
                        
                        if success then
                            isEnemy = result
                        end
                        
                        if isEnemy then
                            shortestDistance = distance
                            closest = otherPlayer
                        end
                    end
                end
            end
        end
    end
    
    return closest
end

local function toggleAimbot(state)
    if aimbotConnection then
        aimbotConnection:Disconnect()
        aimbotConnection = nil
    end
    
    if aimbotCircle then
        aimbotCircle.Visible = state
        aimbotCircle.Radius = aimbotRadius
    else
        createAimbotCircle()
        aimbotCircle.Visible = state
        aimbotCircle.Radius = aimbotRadius
    end
    
    if not state then
        updateStatus("Aimbot Disabled")
        return
    end
    
    aimbotConnection = runService.RenderStepped:Connect(function()
        if not aimbotCircle then 
            createAimbotCircle() 
            aimbotCircle.Radius = aimbotRadius
        end
        aimbotCircle.Position = Vector2.new(mouse.X, mouse.Y)
        aimbotCircle.Radius = aimbotRadius
        aimbotCircle.Visible = aimbotEnabled
        
        local target = getClosestTarget()
        
        if target and target.Character and target.Character:FindFirstChild("Head") then
            local head = target.Character.Head
            local targetPos = head.Position
            local currentPos = camera.CFrame.Position
            local newPos = currentPos:Lerp(targetPos, aimbotSmoothness)
            camera.CFrame = CFrame.new(currentPos, targetPos)
            aimbotCircle.Color = Color3.fromRGB(0, 255, 0)
        else
            aimbotCircle.Color = Color3.fromRGB(255, 0, 255)
        end
    end)
    
    updateStatus("Aimbot Active (Smooth)")
end

